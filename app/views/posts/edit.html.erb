  <h1 class="login_title">トイレ情報の編集</h1>
  <div class="row">
    <div class="col-md-6 col-md-offset-3 section post_col">
    <%= render 'posts/post_form' %>
      <div class="ac-donhave">
        <%= link_to "投稿を削除する", post_path(@post),  method: :delete, data: { confirm: "この投稿を削除してよろしいですか？" }  %>
      </div>
    </div>

  </div>
</div>
